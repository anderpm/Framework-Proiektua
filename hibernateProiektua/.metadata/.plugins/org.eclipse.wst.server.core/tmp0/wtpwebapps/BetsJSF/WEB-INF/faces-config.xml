<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>findQuestionBean</managed-bean-name>
		<managed-bean-class>eredua.bean.FindQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mainBean</managed-bean-name>
		<managed-bean-class>eredua.bean.MainBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createQuestionBean</managed-bean-name>
		<managed-bean-class>eredua.bean.CreateQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>eredua.bean.RegisterBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logInBean</managed-bean-name>
		<managed-bean-class>eredua.bean.LogInBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>defentsaBean</managed-bean-name>
		<managed-bean-class>eredua.bean.DefentsaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logIn</from-outcome>
			<to-view-id>/LogIn.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>LogIn.xhtml</display-name>
		<from-view-id>/LogIn.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/Menua.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menua.xhtml</display-name>
		<from-view-id>/Menua.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>findQuestion</from-outcome>
			<to-view-id>/FindQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menua.xhtml</display-name>
		<from-view-id>/Menua.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createQuestion</from-outcome>
			<to-view-id>/CreateQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menua.xhtml</display-name>
		<from-view-id>/Menua.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>defentsa</from-outcome>
			<to-view-id>/Defentsa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
